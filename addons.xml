<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.wozboxntv64x" name="WOZBOX NTV 64X" version="1.0.0" provider-name="WOZTEC">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">plugin.video.wozboxntv64x</summary>
    <description lang="en">Stream all of your favourites sports, movies and tv from NTV.mx. Requires a registration and paid subscription. Without subscription you can only use Euronews. Please register at wozboxtv.com/registration[CR][CR]Record or display one channel at a time[CR][CR]Record your notifications from Wozbox TVguide and other TV Guides[CR][CR]Record recursively based on text search in your TV guide[CR][CR]Record from dr.dk nu, dr.dk Bonanza and FilmOn with special versions of theese addons[CR][CR]Can now be used with PVR IPTV Simple Client[CR][CR]Version Date 2016-10-19</description>
    <disclaimer lang="en">Wozbox NTV is forked and based on NTV.mx by Mikey1234's original code </disclaimer>
    <platform>all</platform>
  </extension>
  <extension point="xbmc.service" library="service.py" start="login" />
</addon>

<addon id="script.module.futures"
       name="futures"
       version="2.2.0"
       provider-name="Brian Quinlan">
  <requires>
    <import addon="xbmc.python" version="2.24.0"/>
  </requires>
  <extension point="xbmc.python.module"
             library="lib"/>
  <extension point="xbmc.addon.metadata">
    <summary>A futures implementation for Python</summary>
    <description>Backport of the concurrent.futures package from Python 3.2</description>
    <platform>all</platform>
    <license>BSD</license>
    <source>https://code.google.com/p/pythonfutures/source/browse</source>
    <website>https://code.google.com/p/pythonfutures</website>
  </extension>
</addon>

<addon id="script.module.requests"
       name="requests"
       version="2.9.1"
       provider-name="kennethreitz, beenje">
  <requires>
    <import addon="xbmc.python" version="2.24.0"/>
  </requires>
  <extension point="xbmc.python.module"
             library="lib" />
  <extension point="xbmc.addon.metadata">
    <summary>Python HTTP for Humans</summary>
    <description>Packed for KODI from https://github.com/kennethreitz/requests</description>
