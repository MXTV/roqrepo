<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.roqtv" version="4.2" name="ROQ TV" provider-name="ROQ TV">
    <requires>
        <import addon="xbmc.python" version="2.1.0"/>
		<import addon="script.module.addon.common" optional="true"/>
		<import addon="script.module.pytz" />
		<import addon="script.module.dateutil" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="startup">
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>ROQ TV</summary>
        <description>ROQ TV offering only the best</description>
		<website>http://roq-tv.com</website>
        <language>en</language>
        <platform>all</platform>
    </extension>
</addon>

<addon id="repository.roqtv" name="ROQ TV Repo" version="1.5" provider-name="ROQTV">
  <requires>
    <import addon="plugin.video.roqtv"/>
  </requires>
	<extension point="xbmc.addon.repository" name="ROQ TV add-on repository">
		<dir>
		<info compressed="false">http://raw.github.com/MXTV/roqrepo/master/addons.xml</info>
		<checksum>http://raw.github.com/MXTV/roqrepo/master/addons.xml.md5</checksum>
		<datadir zip="true">http://raw.github.com/MXTV/roqrepo/master/</datadir>
		</dir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>ROQ TV Addons</summary>
		<description>Download and install addons from the ROQ TV repository.</description>
		<platform>all</platform>
	</extension>
</addon>

